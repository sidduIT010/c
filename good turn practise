#include <stdio.h>

int main() {
    int t;
    if (scanf("%d", &t) != 1) return 0;
    
    while (t--) {
        int x, y;
        if (scanf("%d %d", &x, &y) != 2) break;
        
        if (x + y > 6) {
            printf("YES\n");
        } else {
            printf("NO\n");
        }
    }
    
    return 0;
}
