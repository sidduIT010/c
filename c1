#include <stdio.h>
#include <stdlib.h>
#include <math.h>

struct triangle
{
	int a;
	int b;
	int c;
};

typedef struct triangle triangle;
void sort_by_area(triangle* tr, int n) {
	     long double *area2 = malloc(n * sizeof(long double));
    if (!area2) exit(1);

    for (int i = 0; i < n; i++) {
        long double a = tr[i].a;
        long double b = tr[i].b;
        long double c = tr[i].c;
        long double s = (a + b + c) / 2.0L;              // use fractional half
        area2[i] = s * (s - a) * (s - b) * (s - c);     // this is (area)^2
    }

    for (int i = 0; i < n - 1; i++) {
        for (int j = 0; j < n - i - 1; j++) {
            if (area2[j] > area2[j + 1]) {
                triangle tmp = tr[j];
                tr[j] = tr[j + 1];
                tr[j + 1] = tmp;

                long double tmp2 = area2[j];
                area2[j] = area2[j + 1];
                area2[j + 1] = tmp2;
            }
        }
    }

    free(area2);
}
int main()
{
	int n;
	scanf("%d", &n);
	triangle *tr = malloc(n * sizeof(triangle));
	for (int i = 0; i < n; i++) {
		scanf("%d%d%d", &tr[i].a, &tr[i].b, &tr[i].c);
	}
	sort_by_area(tr, n);
	for (int i = 0; i < n; i++) {
		printf("%d %d %d\n", tr[i].a, tr[i].b, tr[i].c);
	}
	return 0;
}
